   "source": [
    "# Parse a string of code\n",
    "e1 = parse(\"1 + 1\")\n",
    "\n",
    "# Use expression syntax\n",
    "e2 = :(2 + 3)\n",
    "\n",
    "# Use a quote\n",
    "e3 = quote\n",
    "    x = 1\n",
    "    y = 2\n",
    "    x + y\n",
    "end\n",
    "\n",
    "@show e1\n",
    "@show e2\n",
    "@show e3\n",
    "\n",
    "@show eval(e1)\n",
    "@show eval(e2)\n",
    "@show eval(e3)"
   ]
